@model IEnumerable<Inkopslista.Models.Food>

@{
    ViewBag.Title = "Movies";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Movies</h2>
<table class="table table-bordered table-hover">
    <thead>
    <tr>
        <th>Food</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var food in Model)
    {
        <tr>
            <td>@food.Name</td>
        </tr>
    }
    </tbody>
</table>